{
  "ignoreFile": ".ui-code-insight-ignore",
  "jsFilePathPattern": [
    "./src/**/*.js",
    "./src/**/*.ts",
    "./src/**/*.jsx",
    "./src/**/*.tsx"
  ],
  "htmlFilePathPattern": [
    "**/*.{html,js,ts,jsx,tsx}"
  ],
  "scssFilePathPattern": [
    "**/*.{scss,css,less}"
  ],
  "excludeRules": {
    "eslint": {
      "enabled": true,
      "overrideDefault": false,
      "additionalRules": [
        "my-custom-rule",
        "project-specific-rule"
      ]
    },
    "stylelint": {
      "enabled": true,
      "overrideDefault": false,
      "additionalRules": [
        "my-custom-style-rule"
      ]
    },
    "security": {
      "enabled": true,
      "overrideDefault": false,
      "additionalRules": [
        "custom-security-rule"
      ]
    },
    "performance": {
      "enabled": true,
      "overrideDefault": false,
      "additionalRules": [
        "custom-performance-rule"
      ]
    },
    "accessibility": {
      "enabled": true,
      "overrideDefault": false,
      "additionalRules": [
        "custom-accessibility-rule"
      ]
    },
    "testing": {
      "enabled": true,
      "overrideDefault": false,
      "additionalRules": [
        "custom-testing-rule"
      ]
    },
    "dependency": {
      "enabled": true,
      "overrideDefault": false,
      "additionalRules": [
        "custom-dependency-rule"
      ]
    }
  },
  "ignoreFileConfig": {
    "enabled": true,
    "fallbackToDefault": true,
    "customIgnoreFile": null
  }
} 